Option Explicit
Private T     As Integer
Private i     As Integer
Private UB    As Integer
Private p     As Double
'<:-)Multiple Declarations on one line expanded to one per line
'<:-):WARNING: Scope Changed to Private


Public Sub Command1_Click()

  Dim i  As Integer
  Dim UB As Integer
  Dim T  As Integer
  Dim p  As Double

  Dim SA As String
  Dim SB As String
  Dim SC As String
  Dim SD As String
  Dim SE As String
  Picture1.Cls ' clear our picture box
    On Error Resume Next
    'this is where most of the code is, everything else is pretty pointless
    ' most of that is to make stuff look pretty and everything
    'scaleable axis was kinda fun to do, but a little bit of a pain...
    'comments go to jgeewax@standrews-de.org
    '/ this is copied from the other command1 to make sure i dont do
    '/ anything stupid.
    '/ all of the comments with a slash are redone things!!
    'Call HTX.Clear ' clear our point list '/not used here
    If lblT.Count > 1 Then ' if we have more than one label
        For i = 1 To lblT.UBound 'get all them
            Unload lblT(i) 'and unload them!!!
        Next i
    End If
    If SHP.Count > 1 Then
        For i = 1 To SHP.UBound
            Unload SHP(i)
        Next i
    End If
    UB = Int(frmMain.HTX.ListCount - 1) 'set our upper bound to the # of
    'iterations inputted.
    'make sure its an integer
    '/ redone to go through the list
    For T = 0 To UB 'for 0 to the upper bound
        '/redo calculations? NO!
        '/ simply read the points from the list frmmain.htx!!!
        '/p = (Val(frmMain.Text1(0).Text) - (Val(frmMain.Text1(3).Text) * t + (0.5 * Val(frmMain.Text1(1).Text) * t * t)))
        'this sets our p value with the equation to the left of the
        'about box
        '/ this has been modified by adding frmmain. before some things
        '/ just so it works in this form.
        '============
        p = frmMain.HTX.List(T)
        'if we have a value that is really
        If p < -100 Then
            Exit Sub
        End If
        'far down, then simply stop, we dont want insignificant data
        i = T + 1 ' set our value of i to t + 1 (so we dont get 0 in there..)
        If SHP.Count <= UB + 1 Then
            Load SHP(i)
            With SHP(i)
                .Visible = True
                .Left = Picture1.ScaleWidth / 2
                .Top = Picture1.ScaleHeight - p
            End With 'SHP(i)
        End If
        'set our data to the picture in bright blue
        '/HTX.AddItem (p) 'add the y coordinate (height) to the HTX list box
        'this isnt needed any more because we are taking from htx
        If lblT.Count <= UB + 1 Then ' we are already looping, so
            'if we dont have all of the labels generated yet,
            'make some new ones
            Load lblT(i)
            lblT(i).Visible = True 'make sure theyre visible
        End If
        If i Mod 2 = 1 Then 'if the index is odd, put it to one side
            lblT(i).Left = (Picture1.ScaleWidth / 2) + 10
         ElseIf i Mod 2 = 0 Then 'if its even, put it to the other
            lblT(i).Left = (Picture1.ScaleWidth / 2) - 50
        End If
        lblT(i).Top = Picture1.ScaleHeight - p ' set the position
        '(height) of the label so it's next to the point
        lblT(i).Caption = Round(p, 1) 'set the caption of the label
        'to that points height coordinate
    Next T
    '/ this whole thing just makes the graph reappear from the list in
    '/ frmmain.
    '/ the next part generates a "report" of the data
    '/ ====== REPORT ======== \'
    SA = "Report generated by Free Fall Physics Program " & vbNewLine & "Written in full by JJ Geewax '05" & vbNewLine & "Contact: jgeewax@standrews-de.org"
    For i = 0 To 9
        SB = SB & (i + 1) & "     " & frmMain.HTX.List(i) & vbNewLine
    Next i
    With frmMain
        SC = vbNullString & "The following data was calculated using a constant acceleration for an " & "object dropped from " & .Text1(0).Text & " meters." & vbNewLine & _
            "Only the first 10 are shown." & vbNewLine & _
            SB
        SD = "These calculations were made with the following equations" & vbNewLine & "a = " & .Text1(1).Text & " m/s²" & vbNewLine & "v = at" & vbNewLine & "d = hi - (1/2 at²)" & vbNewLine & "d = hi - (vt + 1/2 at²)" & vbNewLine
        SE = "We used the following values:" & vbNewLine & "Initial Velocity = " & .Text1(3).Text & " m/s" & vbNewLine & "Iterations       = " & .Text1(2).Text & vbNewLine & "Acceleration   = " & .Text1(1).Text & " m/s²" & vbNewLine & "Initial Height   = " & .Text1(0).Text & " m"
    End With 'frmMain
    rtbReport.Text = SA & vbNewLine & vbNewLine & SC & vbNewLine & SD & vbNewLine & SE
    On Error GoTo 0
End Sub

